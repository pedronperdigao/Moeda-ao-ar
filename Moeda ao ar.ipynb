    "import random\n",
    "Cara=input('O que é a Cara?')\n",
    "Coroa=input('O que é a Coroa?')\n",
    "def cointoss():\n",
    "    rand_i = random.randint(0, 1)\n",
    "    outcomes = [Cara, Coroa]\n",
    "    return outcomes[rand_i]\n",
    "# Toss a coin 3 times\n",
    "lançamento = cointoss()\n",
    "print(lançamento)"
